DATABASE_URL="postgresql://postgres:password@localhost:5432/pokedex_fatec?schema=public"

# JWT Secret (mude para uma chave segura em produção)
JWT_SECRET=pokedex_fatec_secret_key_2025

# Configurações opcionais
# CORS_ORIGIN=http://localhost:3000
